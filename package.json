{
  "name": "@drainpixie/reki",
  "version": "1.0.0",
  "type": "module",
  "description": "a lithe undostack",
  "license": "LGPL-3.0",
  "files": [
    "dist",
    "package.json"
  ],
  "main": "./dist/index.js",
