{
  "name": "@drainpixie/reki",
  "version": "1.0.0",
  "type": "module",
  "description": "a lithe undostack",
  "license": "LGPL-3.0",
  "files": [
    "dist",
    "package.json"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "scripts": {
    "build": "tsup",
    "prepack": "pnpm run build",
    "test": "node --import tsx --test src/*.test.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "undostack",
    "undo",
    "stack",
    "history",
    "state"
  ],
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/node": "^22.13.9",
    "eslint": "^9.27.0",
    "tsup": "^8.4.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
